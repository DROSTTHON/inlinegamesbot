{
  "name": "Jepthon Game",
  "description": "يمكنك لعب اي لعبه علئ التجرام بواسطه هاذا السورس.",
  "repository": "https://github.com/jacklul/inlinegamesbot",
  "logo": "https://telegra.ph/file/b4e5dab913e08a72bc6ae.jpg",
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-php"
    }
  ],
  "addons": [
    "heroku-postgresql"
  ],
  "env": {
    "BOT_TOKEN": {
      "description": "توكن بوتك من هنا @BotFather",
      "value": ""
    },
    "BOT_USERNAME": {
      "description": "يوزر البوت بدون علامه @",
      "value": ""
    },
    "BOT_WEBHOOK": {
      "description": "Webhook URL (اسم التطبيق الي خليته اول شي)",
      "value": "https://YOURAPPNAME.herokuapp.com"
    },
    "BOT_SECRET": {
      "description": "متغير سري يستخدم لتأمين ربط الويب",
      "generator": "secret"
    },
    "BOT_ADMIN": {
      "description": "ايدي الادمن",
      "value": "",
      "required": false
    }
  }
}
